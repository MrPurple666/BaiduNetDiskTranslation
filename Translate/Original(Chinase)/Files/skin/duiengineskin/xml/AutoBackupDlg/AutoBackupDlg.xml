<layer title="%str3520%" name="AutoBackupDlg" width="900" height="650" appwin="1" ncRect="0,0,0,0" translucent="1" nomaxsize="1">
    <body>
        <dlg pos="0,0,-0,-0" skin="garbageclean_bg" font="0000:微软雅黑" crtext="FFFFFF">
            <!--标题栏-->
            <caption pos="0,0,-0,@38">
                <dlg name="dlg_logo" pos="0,0,200,-0" show="1" msgtransparent="1">
                    <img pos="10,10" skin="garbageclean_logo"></img>
                    <text name="main_panel_title" pos="39,8" crtext="FFFFFF" font="0200:微软雅黑">%str3520%</text>
                </dlg>
                <imgbtn name="btn_return" pos="10,10,@52,@14" skin="garbageclean_back_btn" class="cls_garbageclean_return_link" 
					txtxoffset="18" cursor="arrow" tip="%str365%" animate="1" show="0">%str365%</imgbtn>
                <imgbtn name="btn_minimize" skin="garbageclean_minimize_btn" pos="-66,4,-34,21" cursor="arrow" tip="%str251%" animate="1"/>
                <imgbtn name="btn_close" skin="garbageclean_close_btn" pos="-34,4,-2,21" cursor="arrow" tip="%str250%" animate="1"/>
            </caption>
            
            <!--功能引导面板-->
            <dlg name="autobackup_guide_panel" pos="1,38,-1,-1" crbg="FFFFFF" show="0">              
                <dlg name="guide_panel_function_img" pos="0,0,-0,@400">
                    <img pos="0,0,-0,@400" skin="auto_backup_function_img"></img>
                </dlg>              
                <dlg name="guide_panel_function_introduction" pos="0,[28,-0,@48">
                  <dlg name="intuction_tip_1" pos="114,0,@144,@48">
                    <text pos="26,0,@100,@24"  crtext="333333" multilines="0" align="left" font="0300:微软雅黑">%str3541%</text>
                    <text pos="0,[0,@144,@24"  crtext="333333" multilines="0" align="left" font="0300:微软雅黑">%str3542%</text>
                  </dlg>
                  <dlg name="intuction_tip_2" pos="[128,0,@128,@48">
                    <text pos="0,0,@128,@24"  crtext="333333" multilines="0" align="left" font="0300:微软雅黑">%str3543%</text>
                    <text pos="6,[0,@108,@24"  crtext="333333" multilines="0" align="left" font="0300:微软雅黑">%str3544%</text>
                  </dlg>
                  <dlg name="intuction_tip_3" pos="[144,0,@112,@48">
                    <text pos="0,0,@112,@24"  crtext="333333" multilines="0" align="left" font="0300:微软雅黑">%str3545%</text>
                    <text pos="0,[0,@112,@24"  crtext="333333" multilines="0" align="left" font="0300:微软雅黑">%str3546%</text>
                  </dlg>
                </dlg>
                <dlg name="guide_panel_bottom" pos="0,[36,-0,-0">
                  <imgbtn name="btn_try_autobackup" pos="330,0,@238,@58" class="autobackup_active_btn">%str3526%</imgbtn>
                </dlg>
            </dlg>
            
            <!--文件夹自动备份操作面板-->
            <dlg name="autobackup_manage_panel" pos="1,38,-1,-1" show="0">
                <include src="IDR_DUI_AUTOBACKUP_VIEW_PANEL"/>
            </dlg>
        </dlg>
    </body>
</layer>