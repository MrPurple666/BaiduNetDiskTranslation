            class="cls_transleftpanel_op_btn" txtxoffset="55">%str1800%</imgbtn>
    <hr name="hr_line_download" pos="0,0,@3,-0" crbg="06a8ff" show="0"/>
  </dlg>
  <dlg pos="0,46,-0,@38">
    <imgbtn name="btn_op_upload" pos="0,0,-0,-0" skin="transpanel_left_upload_btn" 
