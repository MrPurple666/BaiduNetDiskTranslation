        <img name="ad_tip_bkg" pos="0,0,-0,-0"></img>
        <text name="ad_tip" pos="0,0,-0,-0" crtext="ffffff" font2="facename:宋体;bold:0;italic:0;underline:0;adding:-1" textmode="40825"></text>
      </dlg>
